<%- include('partials/header'); -%>
<div class="bod login">
    <div class="row">
        <div class="col-lg-4 col-sm-12 mobileErr">
            <% if(messages.length!=0){ %>
            <%for(let g=0;g<messages.length;g++){ %>
            <div class="alert alert-warning alert-dismissible fade show" role="alert">
                <strong><%= messages[g] %></strong>
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <% } %>
            <% } %>
        </div>
        <div class="col-lg-8 col-sm-12">
            <div class="container socio">
                <div class="design">
                    <div class="reg">
                        <h1>Hey commander show your ID ^_^</h1>
                        <form action="/admin/login" method="POST">
                            <label class="Email" for="exampleFormControlTextarea1"><strong>Email</strong></label>
                            <input type="email" name="username" required="required" class="form-control norad"
                                id="email" autocomplete="email" placeholder="put your email here" autocomplete="off">
                            <label class="Password" for="exampleFormControlTextarea1"><strong>Password</strong></label>
                            <input type="password" required="required" name="password" class="form-control norad"
                                placeholder="enter the password" id="password" autocomplete="off">
                            <label class="Key" for="exampleFormControlTextarea1"><strong>Key</strong></label>
                            <input type="password" required="required" name="key" class="form-control norad"
                                placeholder="enter the key" id="key" autocomplete="off">
                            <button type="submit" class="btn btn-block btn-success gap">Log in</button>
                        </form>
                        <div class="link">
                            <a class="loginLink" href="/login">Not Admin get to the login page, click here.</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-4 col-sm-12 pcErr">
            <% if(messages.length!=0){ %>
            <%for(let g=0;g<messages.length;g++){ %>
            <div class="alert alert-warning alert-dismissible fade show" role="alert">
                <strong><%= messages[g] %></strong>
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <% } %>
            <% } %>
        </div>
    </div>
</div>
</div>
</div>
</div>
<%- include('partials/footer'); -%>