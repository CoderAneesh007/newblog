<%- include('partials/header'); -%>
    <div class="container new">
        <h1>List of all the users</h1>
        <form class="form-inline my-2 my-lg-0" action="/lists" method="POST">
            <input class="form-control mr-sm-2" name="search" type="search" placeholder="Search" aria-label="Search">
            <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
        </form><br>
        <table class="table table-hover mobileTab">
            <thead>
                <th width="50%">Name</th>
                <th width="20%">Status</th>
                <th>link</th>
            </thead>
            <tbody>
                <% list.forEach(obj=>{ %>
                    <tr>
                        <td><%= obj.name %></td>
                        <td>
                            <% if(obj.block!=false){ %>
                                <i class="fas fa-lock" style="color:red;"></i>
                            <% }else{ %>
                                <i class="fas fa-unlock" style="color:green;"></i>
                            <% } %>
                        </td>
                        <td><a href="/adminview/<%= obj._id %>"><button class="btn btn-outline-info btn-sm">click here</button></a></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
        <table class="table table-hover pcTab">
            <thead>
                <th>Name</th>
                <th>ID</th>
                <th>email</th>
                <th>Status</th>
                <th>link</th>
            </thead>
            <tbody>
                <% list.forEach(obj=>{ %>
                    <tr>
                        <td><%= obj.name %></td>
                        <td><%= obj._id %></td>
                        <td><%= obj.email %></td>
                        <td>
                            <% if(obj.block!=false){ %>
                                <i class="fas fa-lock" style="color:red;"></i>
                            <% }else{ %>
                                <i class="fas fa-unlock" style="color:green;"></i>
                            <% } %>
                        </td>
                        <td><a href="/adminview/<%= obj._id %>"><button class="btn btn-outline-info btn-sm">click here</button></a></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
<%- include('partials/footer'); -%>