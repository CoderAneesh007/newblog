<%- include('partials/header'); -%>
<img class="image cover" src="https://www.itl.cat/pngfile/big/40-403156_letter-wallpapers-ibov-jonathandedecker-com.jpg" alt="">
<div class="home"><i class="fas fa-newspaper"></i></div>
<div class="newsBody">
<div class="text-center">
    <form action="/newsLetter" class="form-signin" method="POST">
      <img class="mb-4 column" src="/images/Asset 41.png" alt="">
      <h1 class="h3 mb-3 font-weight-normal">Please sign up</h1>
      <label for="inputEmail" class="sr-only">Email address</label>
      <input type="email" name="email" id="inputEmail" class="form-control top" placeholder="Email address" required autofocus>
      <input type="text" name="Fname"class="form-control mid" placeholder="First name" required autofocus>
      <input type="text" name="Lname" class="form-control bottom" placeholder="Last name" required>
      <button class="buttonSubs btn btn-lg btn-primary btn-block" type="submit">Sign up</button>
      <p class="mt-5 mb-3 text-muted">&copy; T for Tech 2020-2021</p>
    </form>
</div>
<%- include('partials/footer'); -%>
